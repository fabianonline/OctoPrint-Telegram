    	<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
       <nobr><h4><i class="icon-info-sign icon-large" style="color:blue"></i>&nbsp;&nbsp;{{_('Emojis') }}</h4></nobr>
